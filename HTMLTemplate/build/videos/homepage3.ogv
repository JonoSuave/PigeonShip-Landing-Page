    function acceptpackage(id) {

        if ($('#tandc_accept').is(":checked")) {
            $.ajax({
                type: "POST",
                url: BASE_URL + "pigeon_dashboard/acceptpackage/",
                data: {'pid': id},
                cache: false,
                success: function (data) {
                    if (data == 1) {
                        alert('Item accepted succcesfully')
                        window.location.href = BASE_URL + 'pigeon_dashboard/packageslist';
                    } else if (data == 9) {
                        alert('Your account is not approved by admin to accept and deliver the items. Please contact admin.');
                    }
                    else if (data == 2) {
                        alert('You are not eligble to accept this item');
                    } else if (data == 0) {
                        alert('You just missed it!  Item already accepted');
                    } else if (data == 10) {
                        alert('Your account is paused to deliver the items');
                    } else if (data == 4) {
                        alert('Please complete your profile first under edit your profile.');
                        window.location.assign(BASE_URL + "pigeon_dashboard/editprofile")
                    } else {
                        alert('Please login to accept this item');
                    }
                }
            });
        } else {
            alert('Please read and accept the term and conditions.');
        }
    }
